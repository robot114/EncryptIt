        <attr name="clearButtonOnRight" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="DetailTimeLayout">
        <attr name="detailTimeLabelStyle" format="reference"/>
        <attr name="detailTimeStyle" format="reference"/>
